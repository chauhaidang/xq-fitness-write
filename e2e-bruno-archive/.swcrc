{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false,
  "sourceMaps": false,
  "exclude": ["node_modules", "workflows", "test-reports"],
  "stripLeadingPaths": true
}
