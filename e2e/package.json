{
  "name": "xq-fitness-write-service-e2e",
  "version": "1.0.0",
  "description": "E2E workflow tests for XQ Fitness Write Service using Jest + PactumJS",
  "private": true,
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --ci --reporters=default --reporters=jest-junit",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "e2e",
    "testing",
    "api",
    "workflows"
  ],
  "author": "XQ Fitness Team",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.0",
    "@types/wait-on": "^5.3.4",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "pactum": "^3.7.1",
    "ts-jest": "^29.2.0",
    "ts-node": "latest",
    "typescript": "^5.5.4",
    "wait-on": "^8.0.1",
    "@chauhaidang/xq-js-common-kit": "1.0.7",
    "xq-fitness-write-client": "file:../generated-clients/write-service",
    "xq-fitness-read-client": "file:../generated-clients/read-service"
  },
  "jest-junit": {
    "outputDirectory": "./tsr",
    "outputName": "junit.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
